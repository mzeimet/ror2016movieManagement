<% if entity.errors.any? %>
<div id="error_explanation">
    <div class="alert alert-danger" role="alert">
        <p id="notice">
            <h4><%= pluralize(entity.errors.count, "error") %>
                prohibited this
                <%= name%>
                from being saved</h4>
        </p>
    </div>
</div>
<% end %>
